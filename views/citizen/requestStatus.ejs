<%- include("../partials/header") %>
<h2>My Requests</h2>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>ID</th>
      <th>Service</th>
      <th>Status</th>
      <th>Created At</th>
    </tr>
  </thead>
  <tbody>
    <% requests.forEach(r => { %>
      <tr>
        <td><%= r.id %></td>
        <td><%= r.service_id %></td>
        <td><%= r.status %></td>
        <td><%= r.created_at %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
<%- include("../partials/footer") %>
